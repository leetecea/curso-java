package oo.composicao;

public class Item {
	
	String nome;
	int quantidade;
	double preco;
	Compra compra;
	// criar um construtor que jรก recebe os 3 atributos acima
	
	Item(String nome, int quantidade, double preco) {
		this.nome = nome;
		this.quantidade = quantidade;
		this.preco = preco;
	}
}
