package streams;

public class Voluntario {
	final String nome;
	int qtdVoluntariados; 
	String estado;
	
	public Voluntario(String nome, int qtdVoluntariados, String estado) {
		this.nome = nome;
		this.qtdVoluntariados = qtdVoluntariados;
		this.estado = estado;
	} 
	
	public String toString() {
		return nome + " tem nota " ;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((estado == null) ? 0 : estado.hashCode());
		result = prime * result + ((nome == null) ? 0 : nome.hashCode());
		result = prime * result + qtdVoluntariados;
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Voluntario other = (Voluntario) obj;
		if (estado == null) {
			if (other.estado != null)
				return false;
		} else if (!estado.equals(other.estado))
			return false;
		if (nome == null) {
			if (other.nome != null)
				return false;
		} else if (!nome.equals(other.nome))
			return false;
		if (qtdVoluntariados != other.qtdVoluntariados)
			return false;
		return true;
	}

	
}
